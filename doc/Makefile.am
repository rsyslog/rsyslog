# Makefile for rsyslog-doc

SPHINXOPTS    = -j auto
SPHINXBUILD   = sphinx-build
SOURCEDIR     = source
BUILDDIR      = build

# Helper to add parallel jobs from MAKEFLAGS to SPHINXOPTS
# In Automake, MAKEFLAGS is usually available.
# We'll use a shell snippet in the recipes or just rely on sphinx-build's own parallel flag if passed manually.

# However, to be safe and clean in Automake:
html-local:
	@$(SPHINXBUILD) -M html "$(SOURCEDIR)" "$(BUILDDIR)" $(SPHINXOPTS) $(O)
	@echo "Applying Mermaid offline fix..."
	@python3 ./tools/fix-mermaid-offline.py "$(BUILDDIR)/html"

html-with-sitemap:
	@$(SPHINXBUILD) -M html "$(SOURCEDIR)" "$(BUILDDIR)" -t with_sitemap $(SPHINXOPTS) $(O)
	@echo "Applying Mermaid offline fix..."
	@python3 ./tools/fix-mermaid-offline.py "$(BUILDDIR)/html"

singlehtml:
	@$(SPHINXBUILD) -M singlehtml "$(SOURCEDIR)" "$(BUILDDIR)" -t minimal_build $(SPHINXOPTS) -D rst_epilog='' $(O)
	@echo
	@echo "Build finished. The minimal single page HTML is in $(BUILDDIR)/singlehtml."

# 'json' is not a standard target, so we can define it directly.
json:
	@$(SPHINXBUILD) -b json "$(SOURCEDIR)" "$(BUILDDIR)/json" $(SPHINXOPTS) $(O)

alljson: json
	@$(SPHINXBUILD) -b json "$(SOURCEDIR)" "$(BUILDDIR)" $(SPHINXOPTS) $(O)

rag-db: html-local
	@python3 build_rag_db.py

json-formatter: rag-db

clean-local:
	rm -rf "$(BUILDDIR)"

# Original Help target
help:
	@$(SPHINXBUILD) -M help "$(SOURCEDIR)" "$(BUILDDIR)" $(SPHINXOPTS) $(O)

.PHONY: html-local html-with-sitemap singlehtml json alljson rag-db json-formatter clean-local help

